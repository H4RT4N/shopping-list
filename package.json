{
  "name": "shopping-list",
  "version": "1.0.0",
  "description": "MERN stack shopping list app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "backend": "nodemon server",
    "frontend": "npm start --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "node server.js"
  },
  "author": "Harry Tran",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.10.14"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
